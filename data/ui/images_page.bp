
using Gtk 4.0;
using Adw 1;

template ImagesPage : Adw.Bin {
	Gtk.ScrolledWindow {
		Adw.Clamp {
		  styles ["clamp"]


		  Gtk.Box {
		  	orientation: vertical;
			spacing: 10;



			Gtk.Label label {
				styles ["heading"]
				xalign: 0;
				margin-top: 20;
				label: "Installed";
			}


			Gtk.ListBox images_list {
				styles ["boxed-list"]
				valign: start;
				selection-mode: none;
			}
		  }
		}
	}
}