
using Gtk 4.0;
using Adw 1;

template ContainersPage : Adw.Bin {
	Gtk.ScrolledWindow {
		Adw.Clamp {
		  styles ["clamp"]

		  		  Gtk.Box {
		  	orientation: vertical;
			spacing: 10;


			Gtk.Label label {
				styles ["heading"]
				xalign: 0;
				margin-top: 20;
				label: "Containers";
			}

		Gtk.ListBox containers_list {
			styles ["boxed-list"]
			valign: start;
			selection-mode: none;
		}
		}
		}
	}
}